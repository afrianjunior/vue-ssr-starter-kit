{
  "name": "vue2-ssr-starter-kit",
  "description": "A Vue 2 SSR Project",
  "author": "afrianjunior",
  "private": false,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "start:production": "cross-env NODE_ENV=production pm2 start server.js --name='Vue kit Server'",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules"
  },
  "dependencies": {
    "axios-es6": "^0.11.1",
    "chance": "1.0.4",
    "es6-promise": "3.2.1",
    "express": "4.14.0",
    "lru-cache": "4.0.1",
    "serialize-javascript": "1.3.0",
    "vue": "2.1.8",
    "vue-router": "2.1.1",
    "vue-server-renderer": "2.1.8",
    "vuex": "2.1.1",
    "vuex-router-sync": "3.0.0"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-helper-vue-jsx-merge-props": "2.0.2",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-vue-jsx": "3.3.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "cross-env": "1.0.8",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "memory-fs": "0.3.0",
    "node-sass": "3.13.1",
    "sass-loader": "4.1.1",
    "url-loader": "0.5.7",
    "vue-loader": "9.9.5",
    "vue-style-loader": "1.0.0",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-hot-middleware": "2.12.2"
  }
}
